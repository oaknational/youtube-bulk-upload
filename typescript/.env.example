# Google OAuth2 Credentials (Required)
CLIENT_ID=your_google_client_id_here
CLIENT_SECRET=your_google_client_secret_here
REDIRECT_URI=http://localhost:3000/oauth2callback

# Spreadsheet Configuration (Optional)
# Can also be passed as command line argument
SPREADSHEET_ID=your_default_spreadsheet_id
SHEET_RANGE=Sheet1!A:E

# File Paths (Optional - defaults shown)
TOKEN_FILE=./token.json
PROGRESS_FILE=./upload_progress.json
LOG_FILE=./upload_log.txt
TEMP_DIR=./temp

# Debug Mode (Optional)
# DEBUG=youtube-bulk-upload